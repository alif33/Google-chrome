{
    "name": "Fancy toolbar",
    "version": "1.0",
    "description": "Build an Extension!",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "img/icon.jpg",
            "32": "img/icon.jpg",
            "48": "img/icon.jpg",
            "128": "img/icon.jpg"
          }
      },
    "icons": {
         "16": "img/icon.jpg",
         "32": "img/icon.jpg",
         "48": "img/icon.jpg",
         "128": "img/icon.jpg"
     },
    "content_scripts": [
        {
          "matches": ["http://*/*" , "https://*/*"],
          "css": ["toolbar.css"],
          "js": ["jquery.js", "toolbar.js"]
        }
    ],
    "web_accessible_resources": [
    "toolbar.html"
    ],
    "manifest_version": 2
}

